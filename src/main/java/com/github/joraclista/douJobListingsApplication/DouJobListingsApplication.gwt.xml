<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="DouJobListingsApplication">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.github.joraclista.douJobListingsApplication.client.DouJobListingsApplication'/>


    <!-- Specify the app servlets.                   -->
    <servlet path='/DouJobListingsApplicationService'
             class='com.github.joraclista.douJobListingsApplication.server.DouJobListingsApplicationServiceImpl'/>
    <source path='client'/>
    <source path='shared'/>

    <set-property name="user.agent" value="safari" />
</module>